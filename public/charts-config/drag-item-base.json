{
  "propsSchema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "图表名称",
        "type": "string"
      },
      "remarks": {
        "title": "图表备注",
        "type": "string"
      },
      "position": {
        "type": "object",
        "title": "图表位置",
        "properties": {
          "x": {
            "title": "距离左边位置",
            "type": "number",
            "ui:options": {
              "min": 0,
              "max": 100000,
              "addonAfter": "px"
            }
          },
          "y": {
            "title": "距离上面位置",
            "type": "number",
            "ui:options": {
              "min": 0,
              "max": 100000,
              "addonAfter": "px"
            }
          }
        }
      },
      "size": {
        "type": "object",
        "title": "图表大小",
        "properties": {
          "width": {
            "title": "宽度",
            "type": "number",
            "ui:options": {
              "min": 10,
              "max": 100000,
              "addonAfter": "px"
            }
          },
          "height": {
            "title": "高度",
            "type": "number",
            "ui:options": {
              "min": 10,
              "max": 100000,
              "addonAfter": "px"
            }
          }
        }
      }
    }
  }
}
